<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.company.pss.domain">

	<class name="SaleAccount" table="saleaccount">
		<id name="id">
			<generator class="native" />
		</id>
		
		<property name="vDate" />
		<property name="totalSaleAmount" />
		<property name="totalCostAmount" />
		<property name="salePrice" />
		<property name="number" />
		<property name="costPrice" />
		
		<many-to-one name="product" column="product_id"/>
		<many-to-one name="client" column="client_id"/>
		<many-to-one name="saleMan" column="saleMan_id"/>
		
	</class>

</hibernate-mapping>
