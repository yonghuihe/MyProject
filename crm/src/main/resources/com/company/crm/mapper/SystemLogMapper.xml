<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.company.crm.mapper.SystemLogMapper" >
  <resultMap id="BaseResultMap" type="com.company.crm.domain.SystemLog" >
    <id column="id" property="id"  />
	<result column="opIp" property="opIp"  />
	<result column="opTime" property="opTime"  />
	<result column="param" property="param"  />
	<result column="function" property="function"  />
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="long" >
    delete from systemLog where id = #{id}
  </delete>
  <insert id="insert" parameterType="SystemLog" useGeneratedKeys="true" keyProperty="id" >
    insert into systemLog 
    (opIp,opUser_id,opTime,param,function)
    values 
    (#{opIp},#{opUser.id},#{opTime},#{param},#{function})
  </insert>
  <update id="updateByPrimaryKey" parameterType="SystemLog" >
    update systemLog
    set
	    opIp=#{opIp},
	    opUser_id=#{opUser.id},
	    opTime=#{opTime},
	    param=#{param},
	    function=#{function}
    where id = #{id}
  </update>
  <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="long" >
    select * from systemLog where id = #{id}
  </select>
  <select id="selectAll" resultMap="BaseResultMap" >
    select * from systemLog
  </select>
  <sql id="conditionSql">
  	<where>
  	</where>
  </sql>
  <select id="queryByCondictionCount" resultType="long">
  	select count(*) from systemLog
  	<include refid="conditionSql"/>
  </select>
  <select id="queryByConditionResult" resultMap="BaseResultMap">
  	select * from systemLog
  	<include refid="conditionSql"/>
  	limit #{start},#{rows}
  </select>
</mapper>