<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="productPkg" extends="struts-default" namespace="/product" >
        <action name="product_*" class="com.conpany.web.action.ProductAction" method="{1}">
        	<result name="list">/WEB-INF/views/list.jsp</result>
        	<result name="edit">/WEB-INF/views/edit.jsp</result>
        	<result type="redirectAction">/product/product_list</result>
        </action>
    </package>
</struts>
