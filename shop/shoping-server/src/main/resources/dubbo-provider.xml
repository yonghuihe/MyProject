<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
	http://code.alibabatech.com/schema/dubbo
	http://code.alibabatech.com/schema/dubbo/dubbo.xsd
	">
<!-- 
dubbo:service标签的一些属性说明：
     1）interface服务接口的路径
     2）ref引用对应的实现类的Bean的ID
     3）registry向指定注册中心注册，在多个注册中心时使用，值为<dubbo:registry>的id属性，多个注册中心ID用逗号分隔，如果不想将该服务注册到任何registry，可将值设为N/A
     4）register 默认true ，该协议的服务是否注册到注册中心。
 -->
    <dubbo:service interface="com._520it.com.base.service.IProductService" ref="productService"></dubbo:service>
    <dubbo:service interface="com._520it.com.base.service.IProductDescService" ref="productDescService"></dubbo:service>
    <dubbo:service interface="com._520it.com.base.service.ICatalogService" ref="catalogService"></dubbo:service>
    <dubbo:service interface="com._520it.com.base.service.IProductImageService" ref="productImageService"></dubbo:service>
    <dubbo:service interface="com._520it.com.base.service.IProductCatalogPropertyValueService" ref="productCatalogPropertyValueService"></dubbo:service>
    <dubbo:service interface="com._520it.com.base.service.IProductSkuPropertyService" ref="productSkuPropertyService"></dubbo:service>
    <dubbo:service interface="com._520it.com.base.service.IProductSkuService" ref="productSkuService"></dubbo:service>
    <dubbo:service interface="com._520it.com.base.service.ISkuPropertyService" ref="skuPropertyService"></dubbo:service>

</beans>



